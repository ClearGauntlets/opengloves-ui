{
  "applicationId": "com.lucidvr.openglovesui",
  "port": 5000,
  "defaultMode": "window",
  "enableHTTPServer": true,
  "enableNativeAPI": true,
  "url": "/public/",
  "nativeBlockList": [],
  "globalVariables": {
    "TEST": "Test Value"
  },
  "modes": {
    "window": {
      "title": "OpenGloves Configurator",
      "width": 800,
      "height": 600,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/public/favicon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false
    },
    "browser": {},
    "cloud": {}
  },
  "cli": {
    "binaryName": "opengloves-ui",
    "resourcesPath": "public/",
    "clientLibrary": "public/js/neutralino.js"
  }
}